{
  "typescript.tsdk": "node_modules/typescript/lib",
  "gitConfigUser.selectedProfile": "Personal",
  "i18n-ally.localesPaths": ["nuxt-app/i18n/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "de",
  "stylelint.configBasedir": "nuxt-app",
  "stylelint.configFile": "nuxt-app/.stylelintrc.js",
  "stylelint.validate": ["css", "vue", "scss"],
  "cSpell.language": "en,de-DE",
  "cSpell.words": [
    "autodetection",
    "berliner",
    "bucketid",
    "bucketname",
    "cientos",
    "docker-compose",
    "fkey",
    "hypernym",
    "imgproxy",
    "kong",
    "nuxtjs",
    "ossp",
    "pgbouncer",
    "PGRST",
    "postgres",
    "raycaster",
    "rellax",
    "Sometype",
    "sortcolumn",
    "sortorder",
    "supabase",
    "SUPABASE",
    "svgs",
    "tresjs",
    "usename",
    "vueuse",
    "wght"
  ],
  "cSpell.ignorePaths": [
    "/supabase",
    ".env.*",
    "docker-compose.yml",
    "/nginx",
    ".vscode",
    "/nuxt-app/package-lock.json",
    "/nuxt-app/node_modules",
    "/nuxt-app/public",
    "/nuxt-app/tsconfig.json",
    "/nuxt-app/.stylelintrc.js",
    "/nuxt-app/.prettierrc",
    "/nuxt-app/eslint.config.js",
    "/nuxt-app/Dockerfile",
    "/nuxt-app/Dockerfile.dev",
    "/nuxt-app/nuxt.config.ts",
    "/nuxt-app/.nuxt",
    "/nuxt-app/.output"
  ]
}
